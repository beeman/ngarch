{
  "name": "ngArch",
  "version": "2.3.0",
  "description": "Angular application architecture tool, analyzing Angular application and visually displaying the modules, components, services and data models in multiple diagrams",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod",
    "copy_readme": "cpx README.md dist",
    "copy_dist_package": "cpx package-dist.json dist",
    "rename_dist_package": "node -e \"require('fs').rename('dist/package-dist.json', 'dist/package.json', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "copy_files": "npm run copy_readme && npm run copy_dist_package && npm run rename_dist_package",
    "pack": "cd ./dist && npm pack",
    "package": "npm run build && npm run copy_files && npm run pack",
    "publish": "cd ./dist && npm publish",
    "ngarch-server": "node node_modules/ngarch-server/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ngx-pwa/local-storage": "^8.2.4",
    "@types/d3": "^4.13.2",
    "ajv": "^6.12.0",
    "angular-resizable-element": "^3.3.0",
    "angular-tree-component": "^7.2.1",
    "angular2-draggable": "^2.3.2",
    "core-js": "^2.6.11",
    "d3": "^4.13.0",
    "express": "^4.17.1",
    "flatted": "^2.0.1",
    "lodash-es": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "ng-arch-ui": "^0.0.7",
    "ng2-movable": "^0.3.1",
    "ngx-toastr": "^9.2.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.19.0",
    "rxjs": "^6.5.4",
