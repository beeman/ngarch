{
  "name": "ngArch",
  "version": "2.2.5",
  "description": "Angular application architecture tool, analyzing Angular application and visually displaying the modules, components, services and data models in multiple diagrams",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod",
    "copy_readme": "cpx README.md dist",
    "copy_dist_package": "cpx package-dist.json dist",
    "rename_dist_package": "node -e \"require('fs').rename('dist/package-dist.json', 'dist/package.json', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "copy_files": "npm run copy_readme && npm run copy_dist_package && npm run rename_dist_package",
    "pack": "cd ./dist && npm pack",
    "package": "npm run build && npm run copy_files && npm run pack",
    "publish": "cd ./dist && npm publish",
    "ngarch-server": "node node_modules/ngarch-server/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@ngx-pwa/local-storage": "^6.2.5",
    "@types/d3": "^4.13.2",
    "ajv": "^6.10.2",
    "angular-resizable-element": "^3.2.4",
    "angular-tree-component": "^7.2.1",
    "angular2-draggable": "^2.3.2",
    "core-js": "^2.6.9",
    "d3": "^4.13.0",
    "express": "^4.17.1",
    "flatted": "^2.0.1",
    "lodash-es": "^4.17.15",
    "material-design-icons": "^3.0.1",
    "ng-arch-ui": "^0.0.7",
    "ng2-movable": "^0.3.1",
    "ngx-toastr": "^9.2.0",
    "popper.js": "^1.15.0",
    "prismjs": "^1.17.1",
    "rxjs": "^6.5.3",
    "socket.io": "^2.2.0",
